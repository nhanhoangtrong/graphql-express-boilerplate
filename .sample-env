# Development and production related variables
HOST=localhost
PORT=3000
DEBUG=false

# Specifies which origins in CORS whitelist
CORS_ORIGIN=http://localhost:3000,http://localhost:3001

# Default MongoDB connection string
MONGODB_CONNECTION_STRING=mongodb://localhost:27017/graphql_express

# Redis connection string
REDIS_CONNECTION_STRING=redis://localhost:6379/0

# Session related environment variables
SESSION_NAME=session_id
SESSION_SECRET=This is session secret
